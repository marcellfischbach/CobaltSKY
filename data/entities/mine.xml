<entity name="MyEntityName" class="vkEntity">
	
	<entityStates>
	
		<entityState id="0" class="vkDynamicColliderState" root="true">
			<shape resourceMode="shared">${models}/mine.staticmesh:Collision</collision>
			<mass>1.0</mass>
			<friction>10.0</friction>
			<restitution>0.0</restitution>
		</entityState>
	
		<entityState id="1" class="vkStaticMeshState" parentID="0">
			<mesh resourceMode="shared">${models}/mine.staticmesh:Mesh</mesh>
			<materials>
				<material slot="0" slotName="MineBody">${materials}/materials.xml:FieldStone</material>
				<material slot="1" slotName="MinePin">${materials}/materials.xml:FieldStoneRed</material>
			</materials>
		</entityState>
	

	</entityStates>
	

	
</entity>