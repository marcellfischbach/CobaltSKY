<entity name="MyEntityName" class="csEntity">
	
	<entityStates>
	
		<entityState id="0" class="csStaticColliderState" >
			<shape resourceMode="shared">${models}/mine.staticmesh:Collision</shape>
		</entityState>
	
		<entityState id="1" class="csStaticMeshState" parentID="0">
			<mesh resourceMode="shared">${models}/mine.staticmesh:Mesh</mesh>
			<materials>
				<material slot="0" slotName="MineBody">${materials}/materials.xml:FieldStone</material>
				<material slot="1" slotName="MinePin">${materials}/materials.xml:FieldStoneRed</material>
			</materials>
			<clippingRange max="50.0"/>
		</entityState>

	

	</entityStates>
	
	<rootState>0</rootState>

	
</entity>